<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">
    <record id="module_category_aduana" model="ir.module.category">
      <field name="name">Aduana</field>
      <field name="description">Permisos y acceso para procesos aduanales</field>
      <field name="sequence">45</field>
    </record>

    <record id="group_aduana_user" model="res.groups">
      <field name="name">Aduana User</field>
      <field name="category_id" ref="module_category_aduana"/>
      <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_aduana_regulatorio" model="res.groups">
      <field name="name">Aduana Regulatorio Admin</field>
      <field name="category_id" ref="module_category_aduana"/>
      <field name="implied_ids" eval="[(4, ref('modulo_aduana_odoo.group_aduana_user'))]"/>
    </record>
  </data>
</odoo>
